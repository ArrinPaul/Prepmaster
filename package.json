{
  "name": "preppath",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down -v",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "db:generate": "pnpm --filter @preppath/database prisma generate",
    "db:migrate": "pnpm --filter @preppath/database prisma migrate dev",
    "db:seed": "pnpm --filter @preppath/database prisma db seed",
    "db:studio": "pnpm --filter @preppath/database prisma studio",
    "db:reset": "pnpm --filter @preppath/database prisma migrate reset --force"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.1"
}
